{
  "name": "tsk",
  "version": "0.1.0",
  "description": "The TSK programming language",
  "author": "MadProbe#7435",
  "license": "ISC",
  "main": "./build/out/compiler.js",
  "bin": {
    "tskc": "build/out/bin/tskc.js"
  },
  "type": "module",
  "types": "./build/out/lib/compiler.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@types/node": "^14.14.13",
    "babel-preset-minify": "^0.5.1",
    "rollup": "^2.35.1",
    "typescript": "^4.2.0-dev.20201211"
  },
  "peerDependencies": {
    "node-fetch": "^2.6.1"
  },
  "scripts": {
    "build": "tsc && rollup build/out/lib/compiler.js -f iife -n \"compile\" --exports named --file build/bundle.js && babel build/bundle.js --out-file build/bundle.min.js ",
    "update": "npm uninstall @babel/cli @babel/core @types/node babel-preset-minify rollup && npm i @babel/cli @babel/core @types/node babel-preset-minify typescript@next rollup -D",
    "start": "node server.js",
    "globalinstall": "npm pack && npm install -g ./tsk-0.1.0.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadProbe/tsk.git"
  },
  "bugs": {
    "url": "https://github.com/MadProbe/tsk/issues"
  },
  "homepage": "https://github.com/MadProbe/tsk#readme"
}
