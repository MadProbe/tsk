{
	"name": "tskc",
	"version": "0.2.0",
	"description": "Compiler for TSK programming language",
	"author": "MadProbe#7435",
	"license": "ISC",
	"main": "./build/bundle.min.cjs",
	"module": "./build/bundle.min.mjs",
	"exports": {
		".": {
			"import": "./build/bundle.min.mjs",
			"browser": "./build/bundle.min.mjs",
			"import-non-minified": "./build.mjs",
			"require": "./build/bundle.min.cjs"
		}
	},
	"type": "module",
	"bin": {
		"tskc": "build/tskc.mjs"
	},
	"types": "./build/decls/lib/compiler.d.ts",
	"devDependencies": {
		"@types/node": "^15.0.2",
		"rollup": "^2.47.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.30.0",
		"tslib": "^2.2.0",
		"typescript": "^4.5.2"
	},
	"peerDependencies": {
		"node-fetch": "^2.6.1"
	},
	"scripts": {
		"build": "rollup -c && tsc -p src/bin/tsconfig.json",
		"update": "npm i --legacy-peer-deps -D rollup-plugin-typescript2@latest @rollup/plugin-babel@latest @babel/cli@latest @babel/core@latest @types/node@latest babel-preset-minify@latest typescript@latest rollup@latest tslib@latest",
		"start": "node server.js",
		"globalinstall": "npm pack && npm install -g ./tskc-0.2.0.tgz",
		"clearbuild": "rm -rf build/out build/bundle.*",
		"minify": "babel build/bundle.js --out-file build/bundle.min.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MadProbe/tsk.git"
	},
	"bugs": {
		"url": "https://github.com/MadProbe/tsk/issues",
		"discord": "https://discord.gg/5dPuBvZjjx"
	},
	"homepage": "https://github.com/MadProbe/tsk#readme"
}
